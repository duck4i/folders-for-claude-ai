{
    "manifest_version": 3,
    "name": "Folders for Claude.AI",
    "version": "0.1.1",
    "description": "Enables folder upload functionality for Claude.ai",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["https://claude.ai/*"],
        "js": ["content.js"]
      }
    ]
  }